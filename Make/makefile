OBJS = $(addprefix $(OD)/, $(OBJ))

OBJSw32 = $(addprefix $(ODw32)/, $(wOBJ))

OBJSw64 = $(addprefix $(ODw64)/, $(wOBJ))

$(OD)/%.o: $(SD)/%.for
	@mkdir -p $(dir $@)
	$(FC) $(INCLUDE) $(FCFLAGS) $< -o $@

$(ODw32)/%.o: $(SD)/%.for
	@mkdir -p $(dir $@)
	$(FCw32) $(INCLUDEw32) $(FCFLAGS) $< -o $@

$(ODw64)/%.o: $(SD)/%.for
	@mkdir -p $(dir $@)
	$(FCw64) $(INCLUDEw64) $(FCFLAGS) $< -o $@

$(OD)/%.o: $(SD)/%.FOR
	@mkdir -p $(dir $@)
	$(FC) $(INCLUDE) $(FCFLAGS) $< -o $@

$(ODw32)/%.o: $(SD)/%.FOR
	@mkdir -p $(dir $@)
	$(FCw32) $(INCLUDEw32) $(FCFLAGS) $< -o $@

$(ODw64)/%.o: $(SD)/%.FOR
	@mkdir -p $(dir $@)
	$(FCw64) $(INCLUDEw64) $(FCFLAGS) $< -o $@

$(OD)/%.o: $(SD)/%.f90
	@mkdir -p $(dir $@)
	$(FC) $(INCLUDE) $(FCFLAGS) $< -o $@

$(ODw32)/%.o: $(SD)/%.f90
	@mkdir -p $(dir $@)
	$(FCw32) $(INCLUDEw32) $(FCFLAGS) $< -o $@

$(ODw64)/%.o: $(SD)/%.f90
	@mkdir -p $(dir $@)
	$(FCw64) $(INCLUDEw64) $(FCFLAGS) $< -o $@

$(OD)/%.o: $(SD)/%.F90
	@mkdir -p $(dir $@)
	$(FC) $(INCLUDE) $(FCFLAGS) $< -o $@

$(ODw32)/%.o: $(SD)/%.F90
	@mkdir -p $(dir $@)
	$(FCw32) $(INCLUDEw32) $(FCFLAGS) $< -o $@

$(ODw64)/%.o: $(SD)/%.F90
	@mkdir -p $(dir $@)
	$(FCw64) $(INCLUDEw64) $(FCFLAGS) $< -o $@

linux:  $(OBJS)
	@mkdir -p $(BD)
	$(FC) $(FLFLAGS) -o $(BD)/$(EFILE) $(OBJS)

w32:  $(OBJSw32)
	@mkdir -p $(BDw32)
	$(FCw32) $(FLFLAGS) -o $(BDw32)/$(Ew32) $(OBJSw32)

w64:  $(OBJSw64)
	@mkdir -p $(BDw64)
	$(FCw64) $(FLFLAGS) -o $(BDw64)/$(Ew64) $(OBJSw64)

clean:
	@rm -f $(OD)/*.o $(OD)/mod/*.mod $(OD)/*/*.o $(BD)/$(EFILE)

cleanw32:
	@rm -f $(ODw32)/*.o $(ODw32)/mod/*.mod $(ODw32)/*/*.o $(BDw32)/$(Ew32)

cleanw64:
	@rm -f $(ODw64)/*.o $(ODw64)/mod/*.mod $(ODw64)/*/*.o $(BDw64)/$(Ew64)

copy:
	@cp $(BD)/$(EFILE) ~/DSSAT46/

# do not delete this line -- make depend depends on it.
