+	$(FC) -c $(FFLAGS) $^ -o $@
+
+%.o: %.for $(MODULES)
+	$(FC) -c $(FFLAGS) $^ -o $@
+
+%.o: %.FOR $(MODULES)
+	$(FC) -c $(FFLAGS) $^ -o $@
+
+dirs:
+	cd SALUS; make
+	cd ttutil; make
+	cd ORYZA; make
+	cd OP_OBS; make
+
+clean:
+	$(RM) $(TARGET) $(OBJECTS) *.mod
+	cd SALUS; make clean
+	cd ttutil; make clean
+	cd ORYZA; make clean
+	cd OP_OBS; make clean
+
+modules:
