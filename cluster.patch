+FC = ifort
+FFLAGS =  -nowarn -g -std95 -traceback
+TARGET = DSCSM046.EXE
+
+OBJECTS = $(patsubst %.f90, %.o, $(wildcard *.f90)) $(patsubst %.for, %.o, $(wildcard *.for)) $(patsubst %.FOR, %.o, $(wildcard *.FOR)) $(patsubst %.F90, %.o, $(wildcard *.F90))
