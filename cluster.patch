+all: modules dirs $(TARGET)
+
+$(TARGET): $(OBJECTS)
+	$(FC) -o $@ SALUS/*.o ttutil/*.o ORYZA/*.o OP_OBS/*.o $^ $(FFLAGS)
+
+%.o: %.f90 $(MODULES)
+	$(FC) -c $(FFLAGS) $^ -o $@
+
